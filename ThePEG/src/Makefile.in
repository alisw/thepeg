$(DESTDIR)$(bindir)/runThePEG MultiLEP.run;
@USELHAPDF_TRUE@	export LHAPATH=$$(pwd)/$(srcdir)/testpdfs; cd _test_thepeg_installation_; \
@USELHAPDF_TRUE@$(DESTDIR)$(bindir)/setupThePEG --exitonerror $(DESTDIR)$(pkgdatadir)/TestLHAPDF.in; \
